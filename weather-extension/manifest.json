{
    "manifest_version": 3,
    "name": "Weather Extension",
    "version": 1.0,
    "description": "Displays current weather based on your location",
    "permission": [
        "geolocation"
    ],
    "host_permissiom": [
        "htttp://api.openweather.org/"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon":{
        "16": "icons/icon.png",
        "48": "icons/icon.png",
        "128": "icons/icon.png"
    }
    }
}